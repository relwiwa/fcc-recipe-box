{
  "name": "fcc-recipe-box",
  "version": "1.0.0",
  "description": "Recipe Box - Part of Freecodecamp's Data Viz Certification",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && webpack -p",
    "clean": "rimraf build",
    "serve": "webpack-dev-server --config webpack.config.dev.js",
    "deploy:gh-pages": "npm run build && ren build buildx && git checkout gh-pages && rimraf build && ren buildx build && git add . && git commit --message=\"Commit to gh-pages branch\" && git subtree push --prefix build github gh-pages && git checkout master",
    "deploy:surge": "surge -p build"
  },
  "author": "Walter",
  "repository": {
    "type": "git",
    "url": "https://github.com/relwiwa/fcc-recipe-box.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "node-sass": "^4.8.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "foundation-sites": "6.4.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
